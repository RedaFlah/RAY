loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()local a=game:GetService("Players")local b=game:GetService("Debris")local c=Vector3.new(20,20,20)local d=Rayfield:CreateWindow({Name="Hitbox Expander",LoadingTitle="جاري التحميل...",ConfigurationSaving={Enabled=true,FileName="HitboxConfig"}})local e=d:CreateTab("الإعدادات",4483362458)local f=e:CreateToggle({Name="تفعيل الهيت بوكس",CurrentValue=false,Callback=function(g)if g then h()else i()end end})e:CreateSlider({Name="حجم الهيت بوكس",Range={10,50},Increment=1,CurrentValue=20,Callback=function(g)c=Vector3.new(g,g,g)if f.CurrentValue then i()h()end end})local function j(k)local l=k:WaitForChild("HumanoidRootPart")local m=Instance.new("Part")m.Name="Hitbox"m.Size=c m.Transparency=1 m.CanCollide=false m.Anchored=false m.Parent=k Instance.new("Weld",m).Part0=l m.Touched:Connect(function(n)local o=n.Parent if o:FindFirstChild("ArrowTag")then local p=o:FindFirstChild("Shooter")local q=k:FindFirstChild("Humanoid")if p and q and q.Health>0 then q:TakeDamage(30)b:AddItem(o,0.1)end end end)end function h()for r,s in ipairs(a:GetPlayers())do if s.Character then j(s.Character)end end a.PlayerAdded:Connect(function(s)s.CharacterAdded:Connect(j)end)end function i()for r,s in ipairs(a:GetPlayers())do if s.Character and s.Character:FindFirstChild("Hitbox")then s.Character.Hitbox:Destroy()end end end
